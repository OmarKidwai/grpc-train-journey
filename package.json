{
  "name": "grpc-train-journey",
  "version": "1.0.0",
  "description": "Demo of gRPC client and server",
  "keywords": [],
  "author": "Alex Lloyd",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "proto-gen-osx": "npm-run-all proto-gen-client-osx proto-gen-server-osx",
    "proto-gen-win32": "npm-run-all proto-gen-client-win32 proto-gen-server-win32",
    "proto-gen-win64": "npm-run-all proto-gen-client-win64 proto-gen-server-win64",
    "proto-gen-linux": "npm-run-all proto-gen-client-linux proto-gen-server-linux",
    "proto-gen-client-osx": "./protoc/protoc-3.18.1-osx-x86_64/bin/protoc --plugin=\"protoc-gen-ts=./node_modules/.bin/protoc-gen-ts\" --js_out=\"import_style=commonjs,binary:./src/generated/client\" --ts_out=\"service=grpc-web:./src/generated/client\" proto/train_api.proto",
    "proto-gen-server-osx": "./protoc/protoc-3.18.1-osx-x86_64/bin/protoc --plugin=\"protoc-gen-ts=./node_modules/.bin/protoc-gen-ts\" --plugin=\"protoc-gen-grpc=./node_modules/.bin/grpc_tools_node_protoc_plugin\" --js_out=\"import_style=commonjs,binary:./src/generated/server\" --ts_out=\"service=grpc-node,mode=grpc-js:./src/generated/server\" --grpc_out=\"grpc_js:./src/generated/server\" proto/train_api.proto",
    "proto-gen-client-win32": "./protoc/protoc-3.18.1-win32/bin/protoc.exe --plugin=\"protoc-gen-ts=./node_modules/.bin/protoc-gen-ts\" --js_out=\"import_style=commonjs,binary:./src/generated/client\" --ts_out=\"service=grpc-web:./src/generated/client\" proto/train_api.proto",
    "proto-gen-server-win32": "./protoc/protoc-3.18.1-win32/bin/protoc.exe --plugin=\"protoc-gen-ts=./node_modules/.bin/protoc-gen-ts\" --plugin=\"protoc-gen-grpc=./node_modules/.bin/grpc_tools_node_protoc_plugin\" --js_out=\"import_style=commonjs,binary:./src/generated/server\" --ts_out=\"service=grpc-node,mode=grpc-js:./src/generated/server\" --grpc_out=\"grpc_js:./src/generated/server\" proto/train_api.proto",
    "proto-gen-client-win64": "./protoc/protoc-3.18.1-win64/bin/protoc.exe --plugin=\"protoc-gen-ts=./node_modules/.bin/protoc-gen-ts\" --js_out=\"import_style=commonjs,binary:./src/generated/client\" --ts_out=\"service=grpc-web:./src/generated/client\" proto/train_api.proto",
    "proto-gen-server-win64": "./protoc/protoc-3.18.1-win64/bin/protoc.exe --plugin=\"protoc-gen-ts=./node_modules/.bin/protoc-gen-ts\" --plugin=\"protoc-gen-grpc=./node_modules/.bin/grpc_tools_node_protoc_plugin\" --js_out=\"import_style=commonjs,binary:./src/generated/server\" --ts_out=\"service=grpc-node,mode=grpc-js:./src/generated/server\" --grpc_out=\"grpc_js:./src/generated/server\" proto/train_api.proto",
    "proto-gen-client-linux": "./protoc/protoc-3.18.1-linux-x86_64/bin/protoc --plugin=\"protoc-gen-ts=./node_modules/.bin/protoc-gen-ts\" --js_out=\"import_style=commonjs,binary:./src/generated/client\" --ts_out=\"service=grpc-web:./src/generated/client\" proto/train_api.proto",
    "proto-gen-server-linux": "./protoc/protoc-3.18.1-linux-x86_64/bin/protoc --plugin=\"protoc-gen-ts=./node_modules/.bin/protoc-gen-ts\" --plugin=\"protoc-gen-grpc=./node_modules/.bin/grpc_tools_node_protoc_plugin\" --js_out=\"import_style=commonjs,binary:./src/generated/server\" --ts_out=\"service=grpc-node,mode=grpc-js:./src/generated/server\" --grpc_out=\"grpc_js:./src/generated/server\" proto/train_api.proto",
    "webproxy-osx": "./webproxy/grpcwebproxy-v0.14.1-osx-x86_64 --backend_addr=localhost:50051 --run_tls_server=false --allow_all_origins",
    "webproxy-win32": "./webproxy/grpcwebproxy-v0.14.1-win32 --backend_addr=localhost:50051 --run_tls_server=false --allow_all_origins",
    "webproxy-win64": "./webproxy/grpcwebproxy-v0.14.1-win64 --backend_addr=localhost:50051 --run_tls_server=false --allow_all_origins",
    "webproxy-linux": "./webproxy/grpcwebproxy-v0.14.1-linux-x86_64 --backend_addr=localhost:50051 --run_tls_server=false --allow_all_origins",
    "server": "ts-node src/server.ts",
    "client": "webpack serve"
  },
  "devDependencies": {
    "@grpc/grpc-js": "^1.3.7",
    "@improbable-eng/grpc-web": "^0.14.1",
    "@types/node": "^16.10.3",
    "grpc-tools": "^1.11.2",
    "html-webpack-plugin": "^5.3.2",
    "npm-run-all": "^4.1.5",
    "ts-loader": "^9.2.6",
    "ts-node": "^10.2.1",
    "ts-protoc-gen": "^0.15.0",
    "typescript": "^4.4.3",
    "webpack": "^5.58.1",
    "webpack-cli": "^4.9.0",
    "webpack-dev-server": "^4.3.1"
  },
  "dependencies": {
    "google-protobuf": "^3.18.1"
  }
}
